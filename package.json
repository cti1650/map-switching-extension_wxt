{
  "name": "map-switching-extension_wxt",
  "description": "manifest.json description",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@fulcrumapp/tokml": "^0.7.1",
    "@mapbox/togeojson": "^0.16.2",
    "@tmcw/togeojson": "^5.8.1",
    "@xmldom/xmldom": "~0.7.7",
    "classcat": "^5.0.5",
    "csvtojson": "^2.0.10",
    "geojson": "^0.5.0",
    "geojson-to-kml": "^0.0.1",
    "geojson2kml": "^0.1.1",
    "gpx-builder": "^5.6.0",
    "js2xmlparser": "^5.0.0",
    "proj4": "^2.19.10",
    "react": "^18.3.1",
    "react-csv": "^2.2.2",
    "react-dom": "^18.3.1",
    "react-qr-code": "^2.0.18",
    "react-table": "^7.8.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/geojson": "^7946.0.16",
    "@types/proj4": "^2.19.0",
    "@types/react": "^18.3.26",
    "@types/react-csv": "^1.1.10",
    "@types/react-dom": "^18.3.7",
    "@types/react-table": "^7.7.20",
    "@types/xmldom": "^0.1.34",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@wxt-dev/auto-icons": "^1.1.0",
    "@wxt-dev/module-react": "^1.1.5",
    "autoprefixer": "^10.4.21",
    "jsdom": "^27.0.1",
    "lint-staged": "^16.2.5",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "simple-git-hooks": "^2.13.1",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "wxt": "^0.20.11",
    "yarn-audit-fix": "^10.1.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "pre-push": "yarn format"
  },
  "lint-staged": {
    "*.{mjs,js,jsx,ts,tsx,css,json}": "biome format --write --no-errors-on-unmatched",
    "*.{js,jsx,ts,tsx}": "biome check --write --no-errors-on-unmatched"
  },
  "volta": {
    "node": "22.18.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "yarn@1.22.22"
}
